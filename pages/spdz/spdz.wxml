<view class="orders-address" wx:for="{{addressList}}" bindtap="dian" data-index = "{{index}}">
  <text class="orders-address-detail">{{item.add}}</text>

  <text class="orders-address-detail">{{item.number}}</text>
  <view class="mmm">
    <text class="orders-address-name">收货人: {{item.name}}</text>
    <text class="orders-address-phone">电话: {{item.phone}}</text>
  </view>
  <image src="../../images/sc.png" class="mmmm" bindtap="delAddress"></image>
</view>
<navigator url="../address1/address1">
<view class="xg">新增收货地址</view>
</navigator>